         <Pane HBox.hgrow="ALWAYS" />
         <Button fx:id="btnAdmin" text="Admin Paneel" visible="false" />
         <Button fx:id="btnUitloggen" text="Uitloggen" />
      </ToolBar>
      <AnchorPane layoutY="40.0" prefHeight="760.0" prefWidth="1200.0">
         <children>
            <Text fx:id="txt_titel" layoutX="180.0" layoutY="108.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Volgende Wedstrijd">
               <font>
                  <Font size="96.0" />
               </font>
            </Text>
            <Text fx:id="txt_datum" layoutX="180.0" layoutY="200.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Datum">
               <font>
                  <Font size="36.0" />
               </font>
            </Text>
            <Text fx:id="txt_startlocatie" layoutX="180.0" layoutY="250.0" strokeType="OUTSIDE" strokeWidth="0.0" text="StartLocatie">
               <font>
                  <Font size="36.0" />
               </font>
            </Text>
            <Text fx:id="txt_eindlocatie" layoutX="180.0" layoutY="300.0" strokeType="OUTSIDE" strokeWidth="0.0" text="EindLocatie">
               <font>
                  <Font size="36.0" />
