<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.kuleuven.vrolijkezweters.controller.BeheerWedstrijdVerwijderenController">
    <children>
        <Text layoutX="48.0" layoutY="63.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Wedstrijd annuleren">
            <font>
                <Font size="36.0" />
            </font>
        </Text>
        <Button fx:id="btn_verwijderen" layoutX="48.0" layoutY="316.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="94.0" text="Annuleren" />
        <Text layoutX="48.0" layoutY="366.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Wanneer u op deze knop klikt, wordt de wedstrijd (met alle gegevens) definitief verwijderd" />
        <Text layoutX="48.0" layoutY="125.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Selecteer de wedstrijd die u wilt annuleren">
            <font>
                <Font size="18.0" />
            </font>
        </Text>
      <TableView fx:id="table_wedstrijd" layoutX="48.0" layoutY="141.0" prefHeight="154.0">
        <columns>
          <TableColumn fx:id="table_wedstrijddatum" prefWidth="100.0" style="-fx-alignment: CENTER;" text="datum" />
          <TableColumn fx:id="table_wedstrijdnaam" prefWidth="180.0" style="-fx-alignment: CENTER;" text="naam" />
            <TableColumn fx:id="table_wedstrijdstartlocatie" prefWidth="100.0" style="-fx-alignment: CENTER;" text="startlocatie" />
            <TableColumn fx:id="table_wedstrijdeindlocatie" prefWidth="100.0" style="-fx-alignment: CENTER;" text="eindlocatie" />
        </columns>
      </TableView>
    </children>
</AnchorPane>
